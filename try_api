from neuromorpho_api import requestor as requests

current_page = 0
size = 500
base_url = "https://neuromorpho.org/api/neuron"
r = requests.get(f"{base_url}?page={current_page}&size={size}")
# Check that the status_code is 200 and raise if not
r.raise_for_status()
data = r.json()
print(len(data['_embedded']['neuronResources']))
